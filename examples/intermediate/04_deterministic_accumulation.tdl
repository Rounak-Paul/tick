// Deterministic accumulation
// Static variables ensure state is maintained deterministically
func main() {
  static accumulator: int = 0;
  
  accumulator = accumulator + 10;
  println(accumulator);  // Output: 10
  
  accumulator = accumulator + 20;
  println(accumulator);  // Output: 30
  
  accumulator = accumulator + 5;
  println(accumulator);  // Output: 35
}
