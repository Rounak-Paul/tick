// Test program with parallelizable code blocks
func slow_compute1() -> int {
  let sum: int = 0;
  let i: int = 0;
  while (i < 1000) {
    sum = sum + i;
    i = i + 1;
  }
  return sum;
}

func slow_compute2() -> int {
  let sum: int = 0;
  let i: int = 0;
  while (i < 1000) {
    sum = sum + i * 2;
    i = i + 1;
  }
  return sum;
}

func main() {
  // These two can run in parallel (no dependencies)
  let a: int = slow_compute1();
  let b: int = slow_compute2();
  
  // This depends on both a and b
  let c: int = a + b;
  
  println(c);
}
