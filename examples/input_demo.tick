int main() {
    print("Enter your name: ");
    string name = input();
    
    print("Enter your age: ");
    string age_str = input();
    int age = str_to_int(age_str);
    
    print("Enter a decimal number: ");
    string num_str = input();
    float num = str_to_float(num_str);
    
    println(format("Hello, {}!", name));
    println(format("You are {} years old.", age));
    println(format("Your number doubled is: {}", num + num));
    
    return 0;
}
