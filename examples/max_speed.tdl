// Max-speed processing concept (simplified)
// Sequential processing without clock delays

func main() {
  static i: int = 0;
  static processed: int = 0;
  static received: int = 0;
  
  i = i + 1;
  processed = processed + 100;
  received = received + 1;
  
  println(i);
  println(processed);
  println(received);
}

}
