event compute1;
event compute2;
event compute3;
event compute4;
event compute5;

signal<int> s1;
signal<int> s2;
signal<int> s3;
signal<int> s4;
signal<int> s5;

@compute1
process p1 {
  var x : int = 100;
  s1.emit(x);
}

@compute2
process p2 {
  var val : int = s1.recv();
  var y : int = val + 50;
  s2.emit(y);
}

@compute3
process p3 {
  var val : int = s2.recv();
  var z : int = val * 2;
  s3.emit(z);
}

@compute4
process p4 {
  var val : int = s3.recv();
  var w : int = val - 10;
  s4.emit(w);
}

@compute5
process p5 {
  var val : int = s4.recv();
  var result : int = val + 5;
  s5.emit(result);
}

func main() : int {
  compute1.execute();
  compute2.execute();
  compute3.execute();
  compute4.execute();
  compute5.execute();
  
  var final_result : int = s5.recv();
  
  return 0;
}
